{
  "name": "rocketseat-ignite-rentx",
  "version": "1.0.0",
  "description": "Challenge from Rocketseat school to develop a new rent a car code",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn --poll src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/data-source.ts",
    "find-deadcode": "ts-prune",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts",
    "create": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "generate": "typeorm migration:generate -n PostRefactoring",
    "migrate": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -d src/shared/infra/typeorm/data-source.ts",
    "revert": "npx typeorm-ts-node-commonjs migration:revert -d src/data-source",
    "test": "jest"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauloandreola/rocketseat-ignite-rentx.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pauloandreola/rocketseat-ignite-rentx/issues"
  },
  "homepage": "https://github.com/pauloandreola/rocketseat-ignite-rentx#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "csv-parse": "^5.3.1",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
